# Environment Variables for Docker Compose
#
# This file documents how environment variables flow into docker-compose.yml
# Copy this file to .env and fill in the values, then docker-compose will
# automatically load them.
#
# Variables defined here can be referenced in docker-compose.yml using ${VARIABLE_NAME}
# or passed directly to services via the environment section.
#
# Example usage in docker-compose.yml:
#   environment:
#     POSTGRES_USER: ${POSTGRES_USER}
#     POSTGRES_PASSWORD: ${POSTGRES_PASSWORD}
#
# Database Configuration (used by docker-compose services)
POSTGRES_USER=
POSTGRES_PASSWORD=
POSTGRES_DB=
POSTGRES_HOST=
POSTGRES_PORT=

# Backend Configuration
BACKEND_PORT=

# Frontend Configuration
FRONTEND_PORT=
